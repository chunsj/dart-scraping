import os
import glob
from shutil import copyfile

pathList = glob.glob(r"C:\Users\user\Desktop\2\*외부감사실시내용*.*")

for item in pathList:
    filename = os.path.basename(item)
    copyfile(item, os.path.join(r"C:\Users\user\Desktop\pool", filename))
    
